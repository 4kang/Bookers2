<%= render 'list/lists', book: @book, user: @user %>

<h1>Books</h1>
<% if @book.errors.any? %>
 <%= @book.errors.count %>errors prohibited this obj from being saved:
 <ul>
  <% @book.errors.full_messages.each do |message| %>
  <li><%= message %></li>
  <% end %>
 </ul>
<% end %>

<% @books.each do |book| %>
 <div>
  <%= link_to book.title, book_path(book.id) %>

   <p><%= book.body %></p>
 </div>
<% end %>